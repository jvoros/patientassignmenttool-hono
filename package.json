{
  "name": "patientassignmenttool-hono",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev:client": "vite client",
    "dev:server": "node --env-file=.env --watch server/index.js",
    "dev": "concurrently -k -p \"[{name}]\" -n \"client,server\" -c \"bgBlue.bold,bgMagenta.bold\" \"npm run dev:client\" \"npm run dev:server\"",
    "build:client": "vite build client",
    "build": "npm run build:client",
    "preview": "vite preview",
    "core:build": "echo 'building core...' && cd ../patientassignmenttool-core && npm run build",
    "core:copy": "echo 'copying core...' && cp -rv ../patientassignmenttool-core/dist/* ./server/core && cp ../patientassignmenttool-core/dummy/* ./server/core",
    "core:get": "npm run core:build && npm run core:copy",
    "db:production": "npx prisma migrate deploy --schema ./server/prisma/schema.prisma",
    "start": "npm run build && node server/index.js"
  },
  "dependencies": {
    "@hono/node-server": "^1.13.7",
    "@prisma/client": "^6.3.1",
    "@prisma/extension-accelerate": "^1.2.1",
    "@vueuse/core": "^12.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "hono": "^4.6.13",
    "immer": "^10.1.1",
    "radix-vue": "^1.9.11",
    "short-unique-id": "^5.2.0",
    "tailwind-merge": "^2.5.5",
    "tailwindcss-animate": "^1.0.7",
    "vue": "^3.5.13",
    "vue-sonner": "^1.3.0"
  },
  "devDependencies": {
    "@iconify/vue": "^4.3.0",
    "@types/node": "^22.10.1",
    "@vitejs/plugin-vue": "^5.2.1",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.1.0",
    "lucide-vue-next": "^0.468.0",
    "tailwindcss": "^3.4.16",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "unplugin-vue-components": "^0.27.5",
    "vite": "^6.0.1"
  }
}
